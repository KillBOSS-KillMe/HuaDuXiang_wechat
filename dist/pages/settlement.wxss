@import "./../components/mask.wxss";
.red-color {
  color: #ff2228;
}
.gray-color {
  color: #999999;
}
.settlement-wrap {
  background: #eee;
  padding-bottom: 100rpx;
  box-sizing: border-box;
  min-height: 100vh;
  position: relative;
}
.useraddress {
  padding: 20rpx;
  background: #fff;
  position: relative;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.useraddress .imgadd {
  width: 44rpx;
  height: 44rpx;
  margin-right: 24rpx;
}
.useraddress .center {
  flex: 1;
}
.useraddress .center .userinfo {
  font-size: 32rpx;
  color: #292929;
  font-weight: bold;
  padding-bottom: 10rpx;
}
.useraddress .center .useradd {
  font-size: 26rpx;
  color: #999;
  line-height: 1.5;
}
.useraddress .imgright {
  width: 44rpx;
  height: 44rpx;
}
.usergoods-wrap {
  padding-top: 40rpx;
}
.usergoods-wrap .shop-list {
  background: #fff;
  width: 700rpx;
  margin: 0 auto 40rpx;
  border-radius: 20rpx;
}
.usergoods-wrap .shop-list .usergoods {
  padding: 30rpx;
  display: flex;
  align-items: center;
}
.usergoods-wrap .shop-list .usergoods .goodsimg {
  width: 156rpx;
  height: 156rpx;
  border-radius: 10rpx;
  margin-right: 26rpx;
}
.usergoods-wrap .shop-list .usergoods .goods-right {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  height: 160rpx;
}
.usergoods-wrap .shop-list .usergoods .goods-right .goodsname {
  font-weight: bold;
  width: 450rpx;
  font-size: 30rpx;
}
.usergoods-wrap .shop-list .usergoods .goods-right .goodstag {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.usergoods-wrap .shop-list .usergoods .goods-right .goodstag text {
  display: block;
  padding: 0 20rpx;
  background: #fbfbfb;
  border-radius: 6rpx;
  height: 42rpx;
  line-height: 42rpx;
  color: #a3a3a3;
  font-size: 26rpx;
}
.usergoods-wrap .shop-list .usergoods .goods-right .goodstag .goodnum {
  color: #999;
}
.usergoods-wrap .shop-list .usergoods .goods-right .goodprice {
  color: #f92832;
  text-align: right;
}
.usergoods-wrap .shop-list .usergoods .goods-right .goodprice .price {
  font-size: 28rpx;
  font-weight: bold;
}
.usergoods-wrap .shop-list .goods-service {
  padding: 30rpx;
  box-sizing: border-box;
}
.usergoods-wrap .shop-list .goods-service .com-list {
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 70rpx;
}
.usergoods-wrap .shop-list .goods-service .com-list .imgright {
  width: 44rpx;
  height: 44rpx;
}
.usergoods-wrap .shop-list .goods-service .com-list .title {
  font-weight: bold;
}
.usergoods-wrap .shop-list .goods-service .com-list .price {
  flex: 1;
  text-align: right;
}
.usergoods-wrap .shop-list .goods-service .merchandise-subtotal {
  text-align: right;
  height: 88rpx;
  line-height: 88rpx;
  font-weight: bold;
  font-size: 30rpx;
}
.usergoods-wrap .shop-list .goods-service .merchandise-subtotal > text {
  color: #f92832;
}
.settlebottom {
  background: #fff;
  border-top: 1px solid #f2f2f2;
  position: fixed;
  display: flex;
  bottom: 0;
  justify-content: space-between;
  align-items: center;
  z-index: 999;
  height: 100rpx;
  line-height: 100rpx;
  width: 100%;
  padding: 0 30rpx;
  box-sizing: border-box;
}
.settlebottom .priceall {
  color: #a3a3a3;
  font-size: 30rpx;
}
.settlebottom .priceall .allprice {
  color: #f92832;
  font-size: 38rpx;
  font-weight: bold;
}
.settlebottom .sub-btn {
  width: 226rpx;
  height: 70rpx;
  line-height: 70rpx;
  background: #f92832;
  border-radius: 10rpx;
  text-align: center;
  color: #fff;
  font-weight: bold;
  font-size: 30rpx;
}
.earnest-money {
  position: relative;
}
.earnest-money .item {
  display: flex;
  height: 90rpx;
  justify-content: space-between;
  align-items: center;
  padding: 30rpx 30rpx 0;
}
.earnest-money .item:nth-of-type(1) {
  padding-top: 0;
}
.earnest-money .item .title {
  position: relative;
}
.earnest-money .item .price text {
  font-weight: bold;
}
.earnest-money > text {
  height: 90rpx;
  line-height: 90rpx;
  color: #999999;
  padding-left: 30rpx;
}
.price-type {
  background: #fff;
  width: 690rpx;
  border-radius: 10rpx;
  margin: 0 auto;
  color: #333;
  padding: 20rpx;
  box-sizing: border-box;
}
.price-type .price-all {
  text-align: center;
  font-size: 32rpx;
  color: #333;
  padding: 20rpx 0;
  font-weight: bold;
}
.price-type .price-all text {
  color: #f2292d;
}
.price-type .type {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 80rpx;
}
.price-type .type .left {
  font-size: 28rpx;
}
.price-type .type .left switch {
  transform: scale(0.7);
}
.price-type .type .left text {
  color: #666;
}
.price-type .type .price {
  display: flex;
  align-items: center;
}
.price-type .type .price > view {
  color: #f92832;
  margin-left: 10rpx;
  font-weight: bold;
}
.price-type .type .price text {
  color: #666;
}
.price-type .input-wrap {
  display: flex;
  align-items: center;
  height: 80rpx;
}
.price-type .input-wrap > input {
  margin-left: 20rpx;
  border-bottom: 1px solid #ccc;
  width: 400rpx;
  height: 70rpx;
}
.price-type .input-wrap text {
  color: #666;
}

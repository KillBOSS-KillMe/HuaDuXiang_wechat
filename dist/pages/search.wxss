.search-wrap {
  padding: 20rpx 30rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 750rpx;
  box-sizing: border-box;
}
.search-wrap .search-content {
  background: #f6f6f6;
  flex: 1;
  border-radius: 14rpx;
  height: 80rpx;
  display: flex;
  align-items: center;
}
.search-wrap .search-content > image {
  width: 32rpx;
  height: 32rpx;
  margin: 0 20rpx 0 30rpx;
}
.search-wrap .search-content > input {
  flex: 1;
  height: 80rpx;
  font-size: 28rpx;
  color: #000;
}
.search-wrap .search-btn {
  border-radius: 14rpx;
  margin-left: 14rpx;
  background: #5eb320;
  width: 117rpx;
  height: 80rpx;
  text-align: center;
  color: #fff;
  line-height: 80rpx;
}

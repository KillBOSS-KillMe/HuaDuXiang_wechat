
  <view>
    <block  wx:if="{{emptyFlag}}" wx:for="{{5}}" wx:for-index="index" wx:for-item="item" wx:key="key">
      <view class="address-wrap">
        <view class="address-top">
          <view>{{item.name}}名字</view>
          <view>{{item.phone}}13324565655</view>
          <view>默认</view>
        </view>
        <view class="address-text overflow-two">{{item.province}}{{item.city}}{{item.area}}{{item.address}}陕西省西安市蓝田县</view>
        <view class="address-fot">
          <view class="flex-class" bindtap="setAddress" data-id="{{item.id}}">
            <image wx:if="{{true}}" style="width: 48rpx;height: 48rpx;" src="/assets/img/image47.png"/>
            <image wx:else style="width: 48rpx;height: 48rpx;" src="/assets/img/image46.png"/>
            <text>设为默认</text>
          </view>
          <view class="flex-class">
            <navigator hover-class="none" url="/pages/add-address/add-address?id={{item.id}}" class="flex-class bianji">
              <image style="width: 20rpx;height: 20rpx;" src="/assets/img/image45.png"/>
              <view>编辑</view>
            </navigator>
            <view class="flex-class" data-id="{{item.id}}" bindtap="deleAddress">
              <image style="width: 20rpx;height: 20rpx;" src="/assets/img/image44.png"/>
              <view>删除</view>
            </view>
          </view>
        </view>
      </view>
    </block>
    <view class="no-content"  wx:if="{{!emptyFlag}}">
      <image src="/assets/img/image48.png" style="height: 296rpx; width: 296rpx;"/>
      <view>暂无地址</view>
    </view>
    <navigator hover-class="none" url="/pages/addAddress" class="address-btn">添加收货地址</navigator>
  </view>
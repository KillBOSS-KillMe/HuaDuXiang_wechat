.merchant-wrap{min-height:100vh;background:#f9f9f9}.merchant-wrap .merchant-list{display:flex;flex-wrap:wrap;padding:0 30rpx}.merchant-wrap .merchant-list .merchant-item{width:120rpx;height:120rpx;margin:30rpx 70rpx 0 0}.merchant-wrap .merchant-list .merchant-item:nth-of-type(4n){margin-right:0}.merchant-wrap .merchant-list .merchant-item>image{width:100%;height:100%;border-radius:10rpx}